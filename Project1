        "# Changing the markers' size and color\n",
        "plt.figure()\n",
        "plt.plot(accel, speed, '^--k', markersize=10) # shortcut : ms\n",
        "\n",
        "plt.figure() # to plot on another figure\n",
        "plt.plot(accel, speed, '^--k', ms=10, markeredgecolor='r') # shortcut : mec\n",
        "\n",
        "plt.figure()\n",
        "plt.plot(accel, speed, '^--k', ms=10, mec='r', markerfacecolor='b') # shortcut : mfc\n",
        "plt.show()"
      ],
